    cout<<s3<<endl;
    // cout<<s1<s2;
}


#include<stdio.h>
#include<stdlib.h>
int compare(const void * a,const void * b){
    return (*(int*)a < *(int*)b);//desending
    return (*(int*)a > *(int*)b);//acending
}
int main()
{
    int a[]={3,-10,5,2,7,10};
    qsort(a,6,sizeof(int),compare);
    for(int i=0;i<6;i++){
        printf("%d ",a[i]);
    }
}


#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int compare(int i,int j){
    return(i>j);
}
int main()
{
    // vector<int>a(5);
    int a[]={1,4,0,-1,5};
    sort(a,a+5);//ascending
    for(int i=0;i<5;i++){
        cout<<a[i]<<endl;
    }
    cout<<"hehe"<<endl;
    int b[]={4,9,7,0,16};
    sort(b,b+5,compare);//desending
    for(int i=0;i<5;i++){
        cout<<b[i]<<endl;
    }
}


#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
    int a[]={1,2,3,4,5};
    int k=4,l=6;//numbers to be found
    if(binary_search(a,a+5,k)){
        cout<<k<<"is Found"<<endl;
    }
    else
    cout<<k<<" not found"<<endl;
    if(binary_search(a,a+5,l)){
        cout<<l<<" is Found"<<endl;
    }
    cout<<l<<" not found"<<endl;
}


#include<iostream>
#include<algorithm>
int compare(int a,int b){
    return a>b;
}
using namespace std;
int main(){
    int a[]={1,2,3,4,5};
    replace(a,a+5,5,0);
    // sort(a,a+5);
    sort(a,a+5,compare);
    for(int i=0;i<5;i++){
        cout<<a[i]<<" ";
    }
}


#include<iostream>
#include<stack>
#include<string>
#include<cstring>
using namespace std;
int main()
{
    stack<char> s;
    char strg[]="ABCD";
    for(int i=0;i<strlen(strg);i++){
        s.push(strg[i]);
    }
    while(!s.empty()){
        cout<<s.top();
        s.pop();
    }
}


#include<stack>
#include<cstring>
#include<ctype.h>
#include<iostream>
using namespace std;
int main()
{
    char str[]="123*+4-";
    stack<int>n;
    for(int i=0;i<strlen(str);i++){
        char ch=str[i];
        if(isdigit(ch)){
            n.push(ch-'0');
        }
        else{
            int op1=n.top();n.pop();
            int op2=n.top();n.pop();
            switch(ch){
                case '*':n.push(op2*op1);break;
                case '+':n.push(op2+op1);break;
                case '-':n.push(op2-op1);break;
                case '/':n.push(op2/op1);break;
            }
        }
    }
    cout<<"Answer is "<<n.top();
}


#include<iostream>
using namespace std;
int main()
{
    int a=10;
    int &b=a;//a and b has same memory location
    cout<<"a="<<a<<" b="<<b<<endl;
    ++a;
    cout<<"a="<<a<<" b="<<b<<endl;
    ++b;
    cout<<"a="<<a<<" b="<<b<<endl;
}


#include<iostream>
using namespace std;
void function(int &b//call by reference
,int c//call by value
){
    cout<<b<<" "<<&b<<endl;
    cout<<c<<" "<<&c<<endl;
}
int main()
{
    int a=5;
    cout<<a<<" "<<&a<<endl;
    function(a,a);

}


#include<iostream>
using namespace std;
void swap(int &a,int &b){
    int t=a;
    a=b;b=t;
}
int main()
{
    int a=5,b=10;
    cout<<"Before swap a="<<a<<" b="<<b<<endl;
    swap(a,b);
    cout<<"After swap a="<<a<<" b="<<b<<endl;
}


#include<iostream>
using namespace std;
int Return(int &b){//return by value
    cout<<b<<" "<<&b<<endl;
    return b;
}
int& Refreturn(int &b){
    cout<<b<<" "<<&b<<endl;
    return b;
}
int main()
{
    int a=10;
    cout<<a<<" "<<&a<<endl;
    const int &b=Return(a);
    cout<<b<<" "<<&b<<endl;
    //return by reference
    const int &c=Refreturn(a);
    cout<<c<<" "<<&c<<endl;
}
